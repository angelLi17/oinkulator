{
    "manifest_version": 3,
    "name": "oinkulator",
    "version": "0.1.0",
    "description": "piggy calculator decryption",
    "permissions": [
      "scripting"
    ],
    "background": {
      "service_worker": "service_worker.js",
      "type": "module"
    },
    "action": {
      "default_icon": {
        "128": "assets/pig1.png"
      },
      "default_popup": "home.html",
      "default_title": "Open Oinkulator"
    },
    "icons": {
      "128": "assets/pig1.png"
    }
  }
  